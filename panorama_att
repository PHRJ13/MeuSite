		<script>
			function myFunction() {

				//var folder = "assets\\img\\equipe\\";
				var folder = "";

				var today = new Date();
				document.getElementById("dataHoje").innerHTML = today.toLocaleDateString();

				var tur = document.getElementById("turno").value;
				document.getElementById("hora_turno").innerHTML = tur

				var eng = document.getElementById("engenheiro").value;
				document.getElementById('myImage').src = folder + eng;

				switch (eng) {
					case '4003964.jfif': document.getElementById("nome_engenheiro").innerHTML = "Diogo Fran&ccedil;a";
						break;
					case '4010916.jfif': document.getElementById("nome_engenheiro").innerHTML = "Iago Rocha";
						break;
					case '4008025.jfif': document.getElementById("nome_engenheiro").innerHTML = "Josué Silva";
						break;
					case '4002703.jfif': document.getElementById("nome_engenheiro").innerHTML = "Nayara Madeira";
						break;
					case '4010386.jfif': document.getElementById("nome_engenheiro").innerHTML = "Pedro Ribeiro";
						break;
					case '01_desc.jpg': document.getElementById("nome_engenheiro").innerHTML = "Indispon&iacute;vel";
						break;
				}

				switch (eng) {
					case '4003964.jfif': document.getElementById("cel_engenheiro").innerHTML = "Cel: (21) 99957-5036";
						break;
					case '4010916.jfif': document.getElementById("cel_engenheiro").innerHTML = "Cel: (21) 98515-1301";
						break;
					case '4008025.jfif': document.getElementById("cel_engenheiro").innerHTML = "Cel: (21) 98924-1362";
						break;
					case '4002703.jfif': document.getElementById("cel_engenheiro").innerHTML = "Cel: (21) 96468-0246";
						break;
					case '4010386.jfif': document.getElementById("cel_engenheiro").innerHTML = "Cel: (21) 99175-5158";
						break;
					case '01_desc.jpg': document.getElementById("cel_engenheiro").innerHTML = "";
						break;
				}

				switch (eng) {
					case '4003964.jfif': document.getElementById("obs_engenheiro").innerHTML = "Tel: 2588-0036";
						break;
					case '4010916.jfif': document.getElementById("obs_engenheiro").innerHTML = "Tel: 2588-0036";
						break;
					case '4008025.jfif': document.getElementById("obs_engenheiro").innerHTML = "Tel: 2588-0036";
						break;
					case '4002703.jfif': document.getElementById("obs_engenheiro").innerHTML = "Tel: 2588-0036";
						break;
					case '4010386.jfif': document.getElementById("obs_engenheiro").innerHTML = "Tel: 2588-0036";
						break;
					case '01_desc.jpg': document.getElementById("obs_engenheiro").innerHTML = "";
						break;
				}


				var sup = document.getElementById("supervisor").value;
				document.getElementById('myimgsup').src = folder + sup;
				switch (sup) {
					case '01_desc.jpg': document.getElementById("nome_supervisor").innerHTML = "Indispon&iacute;vel";
						break;
					case '3148580.jfif': document.getElementById("nome_supervisor").innerHTML = "Almeida";
						break;
					case '3133834.jfif': document.getElementById("nome_supervisor").innerHTML = "Gerson";
						break;
					case '3149420.png': document.getElementById("nome_supervisor").innerHTML = "Levi";
						break;
					case '3133389.jfif': document.getElementById("nome_supervisor").innerHTML = "M&aacute;rcio";
						break;
					case '3151174.jfif': document.getElementById("nome_supervisor").innerHTML = "Renato";
						break;
				}


				var aj1 = document.getElementById("operador1").value;
				document.getElementById('myImage01').src = folder + aj1;
				switch (aj1) {
					case '01_desc.jpg': document.getElementById("nome_operador_01").innerHTML = "Indispon&iacute;vel";
						break;
					case '3148580.jfif': document.getElementById("nome_operador_01").innerHTML = "Almeida";
						break;
					case '4002353.jfif': document.getElementById("nome_operador_01").innerHTML = "Brito";
						break;
					case '4003154.jfif': document.getElementById("nome_operador_01").innerHTML = "Carlos Eduardo";
						break;
					case '4008452.png': document.getElementById("nome_operador_01").innerHTML = "Cleyton";
						break;
					case '4001677.jfif': document.getElementById("nome_operador_01").innerHTML = "Correia";
						break;
					case '4010261.jfif': document.getElementById("nome_operador_01").innerHTML = "Daniel";
						break;
					case '4002048.jfif': document.getElementById("nome_operador_01").innerHTML = "Erique";
						break;
					case '4005712.jfif': document.getElementById("nome_operador_01").innerHTML = "Everton";
						break;
